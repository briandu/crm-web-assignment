<div class="contacts_container">

<h2>All Contacts</h1>

<ul class="contacts">
<% Contact.all.each do |contact| %>
  <li class="contact_list">
    <div class="contact_info">
      <%= contact.first_name %> <%= contact.last_name %>
    </div>
    <div class="modify">
      <a href="#">Edit</a>|<a href="#">Delete</a>
    </div>
  </li>
<% end %>
</ul>

</div>
